<script>
  const filters = {
    type: 'all',
    categories: [],
    sort: 'latest',
    page: 1
  };

  function fetchProducts() {
    const query = new URLSearchParams({
      type: filters.type,
      categories: filters.categories.join(','),
      sort: filters.sort,
      page: filters.page
    });

    fetch(`/allProducts?${query}`, {
      headers: { 'X-Requested-With': 'XMLHttpRequest' }
    })
      .then(res => res.json())
      .then(data => {
        renderProducts(data.products);
        renderPagination(data.totalPages);
        attachAddToEnquiryListeners();
      })
      .catch(err => console.error('Error fetching products:', err));
  }

  function renderProducts(products) {
    const container = document.getElementById('product-list');
    container.innerHTML = '';

    if (products.length === 0) {
      container.innerHTML = '<div class="col-12 text-center">No products found.</div>';
      return;
    }

    products.forEach(product => {
      const html = `
        <div class="col-lg-4 col-md-6 col-sm-12 pb-1">
          <div class="card product-item border-0 mb-4">
            <div class="card-header product-img overflow-hidden bg-transparent border p-0">
              <img class="img-fluid w-100" src="${product.image[0]}" alt="${product.name}">
            </div>
            <div class="card-body text-center p-0 pt-4 pb-3">
              <a href="/product/${product.slug}">
                <h6 class="text-truncate mb-3">${product.name}</h6>
              </a>
              <div class="d-flex justify-content-center"><h6>MOQ: ${product.moq}</h6></div>
            </div>
            <div class="card-footer d-flex justify-content-between bg-light border">
              <a href="/product/${product.slug}" class="btn btn-sm text-dark p-0">
                <i class="fas fa-eye text-primary mr-1"></i>View Detail
              </a>
              <a href="#" class="btn btn-sm text-dark p-0 add-to-enquiry" data-id="${product._id}">
                <i class="fas fa-shopping-cart text-primary mr-1"></i>Add To Enquiry
              </a>
            </div>
          </div>
        </div>
      `;
      container.insertAdjacentHTML('beforeend', html);
    });
  }

  function renderPagination(totalPages) {
    const container = document.getElementById('pagination');
    container.innerHTML = '';

    for (let i = 1; i <= totalPages; i++) {
      const activeClass = filters.page === i ? 'active' : '';
      container.innerHTML += `<li class="page-item ${activeClass}"><a class="page-link" href="#">${i}</a></li>`;
    }

    document.querySelectorAll('#pagination a').forEach((el, i) => {
      el.addEventListener('click', e => {
        e.preventDefault();
        filters.page = i + 1;
        fetchProducts();
      });
    });
  }

  function attachAddToEnquiryListeners() {
    document.querySelectorAll(".add-to-enquiry").forEach(button => {
      button.addEventListener("click", async (e) => {
        e.preventDefault();
        const productId = button.getAttribute("data-id");

        try {
          const response = await fetch(`/add-to-enquiry/${productId}`, { method: "GET" });

          if (response.ok) {
            button.innerHTML = "Added to Enquiry";
            await updateCartCount();
          } else {
            console.error("Failed to add to enquiry.");
          }
        } catch (err) {
          console.error("Error:", err);
        }
      });
    });
  }

  async function updateCartCount() {
    try {
      const res = await fetch('/enquiry-count');
      const data = await res.json();
      const badge = document.getElementById('cart-count');
      if (badge) {
        badge.textContent = data.count;
      }
    } catch (error) {
      console.error('Error fetching cart count:', error);
    }
  }

  document.querySelectorAll('.product-type').forEach(input => {
    input.addEventListener('change', () => {
      filters.type = input.value;
      filters.page = 1;
      fetchProducts();
    });
  });

  document.querySelectorAll('.category-filter').forEach(input => {
    input.addEventListener('change', () => {
      filters.categories = Array.from(document.querySelectorAll('.category-filter:checked')).map(el => el.value);
      filters.page = 1;
      fetchProducts();
    });
  });

  document.getElementById('sortDropdown').addEventListener('change', (e) => {
    filters.sort = e.target.value;
    filters.page = 1;
    fetchProducts();
  });

  window.addEventListener('DOMContentLoaded', () => {
    fetchProducts();
    updateCartCount();
  });
</script>





