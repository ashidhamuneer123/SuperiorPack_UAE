<script>
    document.addEventListener("DOMContentLoaded", function () {
    document.querySelectorAll(".remove-btn").forEach((removeBtn) => {
        removeBtn.addEventListener("click", async (e) => {
            e.preventDefault();
            const productId = removeBtn.getAttribute("data-prod-id");

            // Remove from sessionStorage
            let stored = JSON.parse(sessionStorage.getItem("enquiryClicked")) || [];
            stored = stored.filter((id) => id !== productId);
            sessionStorage.setItem("enquiryClicked", JSON.stringify(stored));

            // Remove row from DOM
            const productRow = document.getElementById(`product-${productId}`);
            if (productRow) productRow.remove();

            try {
                const response = await fetch(`/remove-from-enquiry/${productId}`, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ prod_id: productId }),
                });

                if (response.ok) {
                    console.log("Product removed from enquiry.");
                    window.location.reload(); // Optional refresh
                    await updateCartCount(); // ✅ Update cart count immediately
                   
                } else {
                    console.error("Failed to remove from server session.");
                }
            } catch (err) {
                console.error("Error removing from server:", err);
            }
        });
    });
});

</script>


<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<script>
  const enquiryForm = document.getElementById('enquiryForm');

  enquiryForm.addEventListener('submit', async (e) => {
    e.preventDefault();

    // Extract form values
    const formData = new FormData(enquiryForm);
    const payload = {
      products: [],
      name: formData.get('name'),
      email: formData.get('email'),
      phone: formData.get('phone'),
      additionalMessage: formData.get('additionalMessage')
    };

    // Collect product rows
    const tableRows = document.querySelectorAll('#enquiry-table-body tr');
    tableRows.forEach((row, index) => {
      const prod_id = row.querySelector('td:nth-child(1)')?.innerText.trim();
      const name = row.querySelector('td:nth-child(2)')?.innerText.trim();
      const moq = row.querySelector('input[name^="products["][name$="[moq]"]')?.value.trim();
      const size = row.querySelector('input[name^="products["][name$="[size]"]')?.value.trim();
      const message = row.querySelector('textarea[name^="products["][name$="[message]"]')?.value.trim();

      if (prod_id && name) {
        payload.products.push({ prod_id, name, moq, size, message });
      }
    });

    // Show loading alert
    Swal.fire({
      title: 'Please wait...',
      text: 'Submitting your enquiry...',
      icon: 'info',
      allowOutsideClick: false,
      showConfirmButton: false,
      didOpen: () => {
        Swal.showLoading();
      }
    });

    try {
      const response = await fetch('/submit-enquiry', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify(payload)
      });

      const result = await response.json();

      if (result.success) {
        Swal.fire({
          title: 'Enquiry Sent!',
          text: result.message,
          icon: 'success',
          timer: 1500,
          showConfirmButton: false
        });

        setTimeout(() => {
          window.location.href = '/';
        }, 1000);
      } else {
        Swal.fire('Oops!', 'Something went wrong. Please try again.', 'error');
      }
    } catch (error) {
      Swal.fire('Error', error.message, 'error');
    }
  });
</script>

<script>
  document.addEventListener("DOMContentLoaded", function () {
    // Function to update cart count
    async function updateCartCount() {
      try {
        const res = await fetch('/enquiry-count');
        const data = await res.json();
        const badge = document.getElementById('cart-count');
        if (badge) {
          console.log("Updating cart count:", data.count); // Log count
          badge.textContent = data.count;
        }
      } catch (error) {
        console.error('Error fetching cart count:', error);
      }
    }

    // Call it once page is loaded
    updateCartCount();

    document.querySelectorAll(".add-to-enquiry").forEach((button) => {
      button.addEventListener("click", async (e) => {
        e.preventDefault();
        const productId = button.getAttribute("data-id");

        try {
          const response = await fetch(`/add-to-enquiry/${productId}`, { method: "GET" });

          if (response.ok) {
            button.innerHTML = "Added to Enquiry"; // Optional: show feedback
            await updateCartCount();  // Update cart count
          } else {
            console.error("Failed to add to enquiry.");
          }
        } catch (err) {
          console.error("Error:", err);
        }
      });
    });
  });
</script>

