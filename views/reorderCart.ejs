    <div class="container mt-4">
        <h3>Your Reorder Cart</h3>
        <% if (reorderProducts.length === 0) { %>
          <div class="alert alert-info">Your reorder cart is empty.</div>
        <% } else { %>
          <form id="reorderForm" action="/reorder-cart/submit" method="POST">
            <table class="table table-bordered text-center mb-0">
              <thead class="bg-secondary text-dark">
                <tr>
                  <th class="d-none">Product ID</th>
                  <th>Product Name</th>
                  <th>Product Image</th>
                  <th>Quantity</th>
                  <th>Description</th>
                  <th>Remove</th>
                </tr>
              </thead>
              <tbody>
                <% reorderProducts.forEach((product, index) => { %>
                  <tr>
                    <td class="d-none">
                      <%= product.prod_id %>
                      <input type="hidden" name="products[<%= index %>][prod_id]" value="<%= product.prod_id %>" />
                    </td>
                    <td>
                      <%= product.name %>
                      <input type="hidden" name="products[<%= index %>][name]" value="<%= product.name %>" />
                    </td>
                    <td>
                      <img 
                      src="<%= product.productImage || 'default.jpg' %>" 
                      alt="Product Image" 
                      class="img-thumbnail"
                      style="max-height: 80px; max-width: 80px; object-fit: cover;"
                    />
                    
                      <input type="hidden" name="products[<%= index %>][productImage]" value="<%= product.productImage %>" />
                    </td>
                    <td>
                      <input type="text" name="products[<%= index %>][moq]" value="<%= product.moq %>" class="form-control form-control-sm" />
                    </td>
                    <td>
                      <textarea name="products[<%= index %>][message]" class="form-control form-control-sm"><%= product.message || '' %></textarea>
                    </td>
                    <td>
                        <button 
                          class="btn btn-outline-danger btn-sm remove-btn" 
                          data-prod-id="<%= product.prod_id %>">
                          <i class="fas fa-times"></i>
                        </button>
                      </td>
                      
                  </tr>
                <% }); %>
              </tbody>
            </table>
            <div class="text-end mt-3">
              <button type="submit" class="modern-btn">Submit Order</button>
            </div>
          </form>
        <% } %>
      </div>
      
    
    
